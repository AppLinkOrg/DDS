            <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.p5}}' style='width:200rpx;height:200rpx' class='margin-5 {{applyinfo.p5==""?"img_null":""}}' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.p5}}'></image>

            <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.p6}}' style='width:200rpx;height:200rpx' class='margin-5 {{applyinfo.p6==""?"img_null":""}}' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.p6}}'></image>

            <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.p7}}' style='width:200rpx;height:200rpx' class='margin-5 {{applyinfo.p7==""?"img_null":""}}' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.p7}}'></image>

            <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.p8}}' style='width:200rpx;height:200rpx' class='margin-5 {{applyinfo.p8==""?"img_null":""}}' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.p8}}'></image>

            <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.p9}}' style='width:200rpx;height:200rpx' class='margin-5 {{applyinfo.p9==""?"img_null":""}}' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.p9}}'></image>

          </view>
        </view>
      </view>



      <view class="asd flex-1" style='width:95%'>
        <view class="flex-row column margin-top-20 " style=' margin-bottom:30rpx'>
          <view class="margin-left-20 flex-row flex-center">
            <image class='icon-25' src='{{uploadpath}}resource/{{res.xianji}}'></image>

            <view class=" margin-left-20 flex-1">终点过磅单</view>

            <!-- <image class='icon-25 margin-right-20' bindtap='' wx:if='{{applyinfo.photo==""}}' src='{{uploadpath}}resource/{{res.tianjia}}'></image> -->

          </view>

          <view class='padding-left-20 margin-20 flex-row   ' style=''>

            <view class='' wx:if='{{applyinfo.photo!=""}}'>
              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo}}'></image>

              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo2}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo2}}'></image>

              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo3}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo3}}'></image>

              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo4}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo4}}'></image>

              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo5}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo5}}'></image>

              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo6}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo6}}'></image>

              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo7}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo7}}'></image>

              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo8}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo8}}'></image>

              <image bindtap='viewPhoto' id='{{uploadpath}}photo/{{applyinfo.photo9}}' style='width:200rpx;height:200rpx' class='margin-5' mode='aspectFill' src='{{uploadpath}}photo/{{applyinfo.photo9}}'></image>

            </view>

          </view>
        </view>


        <view class="asd flex-1" style='width:95%'>
          <view class="flex-row  margin-top-20 " style=' margin-bottom:30rpx'>
            <image class='icon-25 margin-left-20' src='{{uploadpath}}resource/{{res.remarks_icon}}'></image>
            <view class=" margin-left-20">备注</view>
            <view class="margin-left-20 margin-right-20 flex-1 padding-10" wx:if='{{applyinfo.order_remark!="undefined"}}' style='border:1rpx solid #CECFCE;border-radius:10rpx;min-height:100rpx;'>{{applyinfo.order_remark}}</view>
            <view class="margin-left-20 margin-right-20 flex-1 padding-10" wx:if='{{applyinfo.order_remark=="undefined"}}' style='border:1rpx solid #CECFCE;border-radius:10rpx;min-height:100rpx;'></view>
          </view>
          <!-- <image src='http://cmsdev.app-link.org/alucard263096/dds/api/order/qrcode?id={{applyinfo.id}}&filename=a.png'></image> -->
          <view>
